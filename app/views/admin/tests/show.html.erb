<h1> <%= t("tests.show.list", test: @test.title) %> </h1>
  <table>
    <tr>
      <th><%= t("tests.show.body") %></th>
      <th colspan="3"><%= t("common.actions") %></th>
    </tr>

    <ol>
      <%= render partial: 'admin/questions/question', collection: @test.questions %>
    </ol>


  </table>

  <%= link_to t('tests.show.new_question'), new_admin_test_question_path(@test)%>
  <%= link_to t('common.edit'), edit_admin_test_path(@test) %>
  <%= link_to t('common.back'), admin_tests_path %>
