  <h1> List questions of test "<%= @test.title %>" </h1>

  <table>
    <tr>
      <th>Body</th>
      <th colspan="3">Actions</th>
    </tr>

    <ol>
      <%= render partial: 'admin/questions/question', collection: @test.questions %>
    </ol>


  </table>

  <%= link_to 'New question', new_admin_test_question_path(@test)%>
