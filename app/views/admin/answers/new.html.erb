<h1><%= answer_header(@answer)%></h1>

<%= form_with url: admin_question_answers_path, method: 'POST', local: true do |form| %>

<p>
  <%= form.label :body %> <br>
  <%= form.text_field :body %> <br>
</p>
<p><%=form.submit%></p>

<%end%>

<%= link_to 'Back', admin_question_path(@answer.question) %>
